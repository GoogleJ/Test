<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/list_item_bg_press"
    tools:ignore="MissingConstraints,RtlCompat">

    <com.zxjk.duoduo.weight.TitleBar
        android:id="@+id/m_add_my_way_title_bar"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        app:titleBarLeftImage="@drawable/ico_back"
        app:titleBarTextColor="@color/white"
        app:titleBarTitle="@string/m_add_my_way_title_bar" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:background="@color/white"

        android:layout_height="60dp">

        <TextView
            android:id="@+id/m_add_my_way_duoduo_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="13dp"
            android:text="@string/m_add_my_way_duoduo_id"
            android:textColor="@color/login_bg"
            android:textSize="17sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <com.suke.widget.SwitchButton
            android:id="@+id/m_add_my_way_duoduo_switch"
            android:layout_width="43dp"
            android:layout_height="26dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="12dp"
            app:sb_checked="true"
            app:sb_show_indicator="false"  />
        <View
            android:background="@color/m_add_friend_line"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginStart="13dp"
            app:layout_constraintBottom_toBottomOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="60dp">

        <TextView
            android:id="@+id/m_add_my_way_phone_id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="13dp"
            android:text="@string/m_add_my_way_phone_id"
            android:textColor="@color/login_bg"
            android:textSize="17sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <com.suke.widget.SwitchButton
            android:id="@+id/m_add_my_way_phone_switch"
            android:layout_width="43dp"
            android:layout_height="26dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="12dp"
            app:sb_checked="false"
            app:sb_show_indicator="false"  />
        <View
            android:background="@color/m_add_friend_line"
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_marginStart="13dp"
            app:layout_constraintBottom_toBottomOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="60dp">

        <TextView
            android:id="@+id/m_add_my_way_group_chat_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="13dp"
            android:text="@string/m_add_my_way_group_chat_text"
            android:textColor="@color/login_bg"
            android:textSize="17sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <com.suke.widget.SwitchButton
            android:id="@+id/m_add_my_way_group_switch"
            android:layout_width="43dp"
            android:layout_height="26dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="12dp"
            app:sb_checked="false"
            app:sb_show_indicator="false"  />
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/m_add_friend_line"
            android:layout_marginStart="13dp"
            app:layout_constraintBottom_toBottomOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="60dp">

        <TextView
            android:id="@+id/m_add_my_way_qrcode_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="13dp"
            android:text="@string/m_add_my_way_qrcode_text"
            android:textColor="@color/login_bg"
            android:textSize="17sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <com.suke.widget.SwitchButton
            android:id="@+id/m_add_my_way_qrcode_switch"
            android:layout_width="43dp"
            android:layout_height="26dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="12dp"
            app:sb_checked="false"
            app:sb_show_indicator="false"  />
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/m_add_friend_line"
            android:layout_marginStart="13dp"
            app:layout_constraintBottom_toBottomOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:background="@color/white"
        android:layout_height="60dp">

        <TextView
            android:id="@+id/m_add_my_way_business_carde_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="13dp"
            android:text="@string/m_add_my_way_business_carde_text"
            android:textColor="@color/login_bg"
            android:textSize="17sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
        <com.suke.widget.SwitchButton
            android:id="@+id/m_add_my_way_business_card_switch"
            android:layout_width="43dp"
            android:layout_height="26dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="12dp"
            app:sb_checked="false"
            app:sb_show_indicator="false"  />
        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/m_add_friend_line"
            android:layout_marginStart="13dp"
            app:layout_constraintBottom_toBottomOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/m_add_my_way_caveat_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/m_add_my_way_caveat_label"
        android:layout_marginStart="12dp"
        android:layout_marginTop="10dp"
        />


</LinearLayout>