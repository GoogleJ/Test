<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/m_item_search_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <ImageView
        android:id="@+id/m_item_search_icon"
        android:layout_width="53dp"
        android:layout_height="53dp"
        android:src="@drawable/icon_header"
        android:layout_marginTop="5dp"
        android:layout_marginStart="7dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="RtlCompat" />
    <TextView
        android:id="@+id/m_item_search_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginStart="13dp"
        android:text="不爱吃水果"
        android:textSize="16dp"
        android:textColor="@color/black"
        app:layout_constraintStart_toEndOf="@+id/m_item_search_icon"
        app:layout_constraintTop_toTopOf="@+id/m_item_search_icon"
        tools:ignore="RtlCompat" />
    <TextView
        android:id="@+id/m_item_search_dudu_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="嘟嘟号：105369"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="@+id/m_item_search_text"
        app:layout_constraintTop_toBottomOf="@+id/m_item_search_text"
        android:layout_marginTop="10dp"
        />

</androidx.constraintlayout.widget.ConstraintLayout>